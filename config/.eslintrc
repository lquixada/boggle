{
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 0,
    "guard-for-in": 0,
    "wrap-iife": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 0,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 0,
    "no-unused-vars": 0,
    "strict": 0,
    "max-params": 0,
    "max-depth": [
      2,
      4
    ],
    "max-statements": [
      2,
      13
    ],
    "complexity": [
      2,
      6
    ],
    "max-len": 0,
    "semi": 0,
    "no-cond-assign": 0,
    "no-debugger": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-unused-expressions": 0,
    "block-scoped-var": 0,
    "no-iterator": 0,
    "linebreak-style": 0,
    "comma-style": [
      2,
      "last"
    ],
    "no-loop-func": 0,
    "no-multi-str": 2,
    "no-proto": 0,
    "no-script-url": 0,
    "no-shadow": 0,
    "dot-notation": 2,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-invalid-this": 0,
    "space-before-blocks": [
      2,
      "always"
    ],
    "one-var": [
      2,
      "never"
    ],
    "spaced-comment": [
      2,
      "always"
    ],
    "yoda": [
      2,
      "never"
    ],
    "consistent-this": [
      2,
      "that"
    ],
    "eol-last": 2,
    "brace-style": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2
  }
}

{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push origin master:heroku",
    "prepush": "npm test",
    "prestart:prod": "npm run build",
    "test": "npm-run-all --parallel test:mocha test:lint",
    "test:mocha": "mocha --compilers js:babel-core/register --require tests/setup.js 'tests/**/*.spec.js'",
    "test:lint": "eslint --ignore-pattern '/public/' --quiet '**/*.js'",
    "test:watch": "npm test -- --watch --growl",
    "build": "webpack -p --progress --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:debug": "NODE_ENV=development node-debug --save-live-edit --no-debug-brk server.js",
    "start:dev": "NODE_ENV=development nodemon --watch ./server server",
    "start:prod": "NODE_ENV=production node server"
  },
  "engines": {
    "node": "6.2.1"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-jsonp": "^1.0.0",
    "hbs": "^4.0.0",
    "morgan": "1.7.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.2.2",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "copy-webpack-plugin": "3.0.1",
    "enzyme": "2.4.1",
    "eslint": "3.1.0",
    "husky": "0.11.5",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "node-inspector": "0.12.8",
    "nodemon": "1.9.2",
    "npm-run-all": "^2.3.0",
    "react-addons-test-utils": "15.2.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

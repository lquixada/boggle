{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push origin master:heroku",
    "prepush": "npm test",
    "prestart:dev": "npm run build:dev",
    "prestart:prod": "npm run build:prod",
    "test": "npm-run-all --parallel test:mocha test:lint",
    "test:mocha": "mocha",
    "test:lint": "npm-run-all --parallel test:lint:js test:lint:sass",
    "test:lint:js": "eslint .",
    "test:lint:sass": "sass-lint --no-exit --verbose",
    "build:dev": "webpack --progress --config webpack.dev.config.js",
    "build:prod": "webpack -p --progress --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:dev": "pm2 start process.yml --only boggle-dev",
    "start:prod": "pm2 start process.yml --only boggle-prod",
    "stop": "pm2 kill",
    "outdated": "npm-check-updates",
    "update": "npm-check-updates -u"
  },
  "engines": {
    "node": "6.2.1"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "clean-webpack-plugin": "0.1.10",
    "css-loader": "0.23.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "fetch-jsonp": "1.0.1",
    "helmet": "2.3.0",
    "morgan": "1.7.0",
    "node-sass": "3.8.0",
    "npm-run-all": "3.0.0",
    "pm2": "2.0.18",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-github-fork-ribbon": "0.4.4",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "sw-precache-webpack-plugin": "0.5.1",
    "underscore": "1.8.3",
    "webpack": "2.1.0-beta.20",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-enzyme": "0.5.0",
    "chai-immutable": "1.6.0",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "husky": "0.11.6",
    "jsdom": "9.4.2",
    "mocha": "3.0.2",
    "node-inspector": "0.12.8",
    "npm-check-updates": "2.8.0",
    "react-addons-test-utils": "15.3.1",
    "react-hot-loader": "1.3.0",
    "sass-lint": "1.9.0",
    "sinon": "1.17.5",
    "supertest": "2.0.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

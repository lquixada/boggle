{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "all": "npm run test && npm run lint",
    "deploy": "git push origin master:gh-pages",
    "prepush": "npm run all",
    "test": "mocha tests/**/*.spec.js --compilers js:babel-core/register --require tests/setup.js",
    "test:watch": "npm test -- --watch --growl",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --hot --inline --history-api-fallback",
    "build": "webpack -p --config config/webpack.prod.config.js",
    "lint": "eslint -c config/.eslintrc **/*.js"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "fetch-jsonp": "^1.0.0",
    "husky": "^0.11.4",
    "jsdom": "^9.2.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "null-loader": "^0.1.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

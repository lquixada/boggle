{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push origin master:heroku",
    "prepush": "npm test",
    "pretest": "npm run build:dev",
    "prestart:dev": "npm run build:dev",
    "prestart:prod": "npm rebuild node-sass && npm run build:prod",
    "test": "npm-run-all --parallel test:mocha test:lint",
    "test:mocha": "nyc mocha",
    "test:lint": "npm-run-all --parallel test:lint:js test:lint:sass",
    "test:lint:js": "eslint .",
    "test:lint:sass": "sass-lint --no-exit --verbose",
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.dev.config.js",
    "build:prod": "webpack -p --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:dev": "pm2 start process.yml --only boggle-dev --no-daemon",
    "start:prod": "pm2 start process.yml --only boggle-prod --no-daemon"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "body-parser": "1.17.1",
    "clean-webpack-plugin": "0.1.11",
    "compression": "1.6.2",
    "css-loader": "0.28.0",
    "es6-promise": "2.3.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.1.0",
    "fetch-jsonp": "1.0.2",
    "helmet": "2.3.0",
    "immutable": "3.8.1",
    "lodash": "4.16.3",
    "morgan": "1.7.0",
    "node-sass": "4.5.2",
    "npm-run-all": "3.1.0",
    "pm2": "2.4.6",
    "react": "15.5.4",
    "react-addons-shallow-compare": "15.3.2",
    "react-dom": "15.5.4",
    "react-github-fork-ribbon": "0.4.4",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-raven-middleware": "1.1.0",
    "redux-thunk": "2.1.0",
    "sass-loader": "6.0.3",
    "style-loader": "0.13.1",
    "sw-precache-webpack-plugin": "0.5.1",
    "webpack": "2.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "chai-immutable": "1.6.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "husky": "0.13.3",
    "jsdom": "10.1.0",
    "mocha": "3.3.0",
    "nyc": "10.3.0",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "1.3.1",
    "sass-lint": "1.10.2",
    "sinon": "2.2.0",
    "supertest": "3.0.0",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "engines": {
    "node": "7.3"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

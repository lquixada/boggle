{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push origin master:heroku",
    "prepush": "npm test",
    "prestart:prod": "npm run build",
    "test": "npm-run-all --parallel test:mocha test:lint",
    "test:mocha": "mocha --compilers js:babel-core/register --require tests/setup.js 'tests/**/*.spec.js'",
    "test:lint": "npm-run-all --parallel test:lint:js test:lint:sass",
    "test:lint:js": "eslint --ignore-pattern '/public/' --quiet '**/*.js'",
    "test:lint:sass": "sass-lint --no-exit --verbose 'styles/**/*.scss'",
    "build": "webpack -p --progress --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:debug": "NODE_ENV=development node-debug --save-live-edit --no-debug-brk server.js",
    "start:dev": "NODE_ENV=development nodemon --watch ./server server",
    "start:prod": "NODE_ENV=production node server"
  },
  "engines": {
    "node": "6.2.1"
  },
  "dependencies": {
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "css-loader": "0.23.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-jsonp": "1.0.0",
    "hbs": "4.0.0",
    "morgan": "1.7.0",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "underscore": "1.8.3",
    "webpack": "1.13.1",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "copy-webpack-plugin": "3.0.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.2",
    "husky": "0.11.6",
    "jsdom": "9.4.1",
    "mocha": "3.0.0",
    "node-inspector": "0.12.8",
    "nodemon": "1.10.0",
    "react-addons-test-utils": "15.3.0",
    "react-hot-loader": "1.3.0",
    "sass-lint": "1.8.2",
    "sinon": "1.17.5",
    "supertest": "2.0.0",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

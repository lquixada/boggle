{
  "name": "boggle",
  "version": "1.0.26",
  "private": true,
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "author": "Leonardo Quixada <lquixada@gmail.com>",
  "license": "MIT",
  "main": "src/desktop/index.js",
  "scripts": {
    "prelambda:deploy": "npm run -s web:build",
    "android:build": "react-native run-android",
    "android:eject": "react-native eject",
    "android:start": "react-native start --config ../../../../rn-cli.config.js",
    "build": "npm run -s web:build",
    "build:analyze": "./tasks/analyze",
    "build:client": "webpack-cli -p --env.prod --config webpack.config.client.js",
    "build:server": "babel -D src -d web",
    "deploy:major": "npm version major && git push --follow-tags",
    "deploy:minor": "npm version minor && git push --follow-tags",
    "deploy:patch": "npm version patch && git push --follow-tags",
    "electron:build": "./tasks/electron-build",
    "electron:start": "NODE_ENV=development electron .",
    "flameon": "./tasks/flame",
    "ios:build": "react-native run-ios",
    "ios:eject": "react-native eject",
    "ios:start": "react-native start --config ../../../../rn-cli.config.js",
    "lambda:deploy": "serverless deploy -v",
    "start": "npm run web:prod",
    "test": "npm run -s test:jest && npm run -s test:lint",
    "test:jest": "jest --coverage",
    "test:lint": "standard",
    "test:codecov": "codecov",
    "web:build": "npm run -s build:server && npm run -s build:client",
    "web:dev": "nodemon --config nodemon.dev.json src/app.js",
    "web:prod": "nodemon web/app.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": "jest"
  },
  "dependencies": {
    "aws-serverless-express": "3.3.5",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cross-fetch": "3.0.0",
    "es6-promise": "4.2.5",
    "express": "4.16.4",
    "helmet": "3.15.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "morgan": "1.9.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-component-octicons": "1.8.0",
    "react-dom": "16.7.0",
    "react-github-fork-ribbon": "0.6.0",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.6.3",
    "react-native": "0.57.8",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "4.0.1",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.3.0",
    "styled-components": "3.4.5",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "assets-webpack-plugin": "3.9.7",
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.1.0",
    "copy-webpack-plugin": "4.6.0",
    "electron": "2.0.8",
    "electron-installer-dmg": "2.0.0",
    "electron-packager": "12.1.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "flux-standard-action": "2.0.3",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "7.2.2",
    "metro": "0.51.1",
    "nock": "9.6.1",
    "node-zip": "1.1.1",
    "nodemon": "1.18.9",
    "react-test-renderer": "16.7.0",
    "regenerator-runtime": "0.13.1",
    "serverless": "1.36.1",
    "serverless-offline": "3.33.0",
    "serverless-s3-deploy": "0.8.0",
    "serverless-webpack": "5.2.0",
    "standard": "12.0.1",
    "supertest": "3.4.1",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.5.0",
    "webpack-hot-middleware": "2.24.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  }
}

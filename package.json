{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push -f origin master:heroku",
    "prepush": "npm test",
    "prestart:prod": "npm rebuild node-sass && npm run build",
    "test": "run-p jest lint",
    "jest": "jest",
    "lint": "run-p lint:js lint:sass",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint --no-exit --verbose",
    "build": "webpack -p --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:dev": "pm2 start process.yml --only boggle-dev --no-daemon",
    "start:prod": "pm2 start process.yml --only boggle-prod --no-daemon"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.17.2",
    "clean-webpack-plugin": "0.1.16",
    "compression": "1.7.0",
    "css-loader": "0.28.4",
    "es6-promise": "4.1.1",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "3.0.0",
    "fetch-jsonp": "1.1.1",
    "helmet": "3.8.1",
    "history": "4.6.3",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "morgan": "1.8.2",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "pm2": "2.6.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-github-fork-ribbon": "0.5.0",
    "react-redux": "5.0.5",
    "react-router-config": "1.0.0-beta.3",
    "react-router-dom": "4.1.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "webpack": "3.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "chai-enzyme": "0.8.0",
    "chai-immutable": "1.6.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "jsdom": "11.1.0",
    "react-hot-loader": "1.3.1",
    "react-test-renderer": "15.6.1",
    "sass-lint": "1.10.2",
    "sinon": "2.4.1",
    "supertest": "3.0.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "engines": {
    "node": "8.2.1"
  },
  "author": "Leonardo Quixad√°",
  "license": "UNLICENSED",
  "jest": {
    "moduleNameMapper": {
      "\\.scss": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>src/**/*.spec.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/__tests__/_setup/index.js"
  }
}

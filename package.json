{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "author": "Leonardo Quixada <lquixada@gmail.com>",
  "license": "MIT",
  "main": "src/desktop/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "predocker:build": "npm run -s web:build",
    "preheroku:push": "heroku login && heroku container:login",
    "analyze": "./tasks/analyze",
    "build": "npm run -s build:platform && npm run -s build:bundle",
    "build:bundle": "NODE_ENV=production webpack-cli -p",
    "build:platform": "babel -D -q src -d $PLATFORM",
    "codecov": "codecov",
    "dev": "npm run -s web:dev",
    "docker:build": "docker build -t lquixada/boggle .",
    "docker:pull": "docker pull lquixada/boggle",
    "docker:push": "docker push lquixada/boggle",
    "docker:start": "docker run -t -i -p 3000:3000 lquixada/boggle",
    "electron:build": "PLATFORM=desktop npm run -s build",
    "electron:package": "electron-forge package --outDir=desktop",
    "electron:make": "electron-forge make",
    "electron:start": "electron-forge start",
    "flameon": "./tasks/flame",
    "heroku:update": "docker tag lquixada/boggle registry.heroku.com/bogglejs/web",
    "heroku:push": "docker push registry.heroku.com/bogglejs/web",
    "heroku:deploy": "npm run -s docker:build && npm run -s heroku:update && npm run -s heroku:push",
    "jest": "jest",
    "jest:cov": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore .",
    "start": "npm run -s web:start",
    "test": "npm run -s jest && npm run -s lint",
    "web:build": "PLATFORM=web npm run -s build",
    "web:dev": "pm2 start .process.yml --only boggle-dev --no-daemon",
    "web:start": "pm2 start .process.yml --only boggle-prod --no-daemon"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cross-fetch": "2.1.1",
    "es6-promise": "4.2.4",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "lodash": "4.17.10",
    "morgan": "1.9.0",
    "pm2": "2.10.3",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-github-fork-ribbon": "0.5.1",
    "react-hot-loader": "4.1.2",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "4.0.0",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.2.0",
    "styled-components": "3.2.6",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autocannon": "2.4.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "clinic": "0.8.1",
    "codecov": "3.0.1",
    "copy-webpack-plugin": "4.5.1",
    "electron": "2.0.0",
    "electron-compile": "6.4.2",
    "electron-forge": "5.2.0",
    "electron-packager": "12.0.2",
    "electron-prebuilt-compile": "2.0.0",
    "electron-squirrel-startup": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.19.1",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-react": "7.7.0",
    "flux-standard-action": "2.0.2",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.1.0",
    "nock": "9.2.5",
    "react-test-renderer": "16.3.2",
    "supertest": "3.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "webpack": "4.7.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "engines": {
    "node": "8.11.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}

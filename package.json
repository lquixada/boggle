{
  "name": "boggle",
  "version": "0.0.1",
  "description": "A web-based implementation of the Boggle game for React study purposes.",
  "scripts": {
    "deploy": "git push origin master:heroku",
    "prepush": "npm test",
    "pretest": "npm run build:dev",
    "prestart:dev": "npm run build:dev",
    "prestart:prod": "npm rebuild node-sass && npm run build:prod",
    "test": "npm-run-all --parallel test:mocha test:lint",
    "test:mocha": "nyc mocha",
    "test:lint": "npm-run-all --parallel test:lint:js test:lint:sass",
    "test:lint:js": "eslint .",
    "test:lint:sass": "sass-lint --no-exit --verbose",
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.dev.config.js",
    "build:prod": "webpack -p --config webpack.prod.config.js",
    "start": "npm run start:prod",
    "start:dev": "pm2 start process.yml --only boggle-dev --no-daemon",
    "start:prod": "pm2 start process.yml --only boggle-prod --no-daemon"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.17.1",
    "clean-webpack-plugin": "0.1.16",
    "compression": "1.6.2",
    "css-loader": "0.28.1",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "fetch-jsonp": "1.0.6",
    "helmet": "3.5.0",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "morgan": "1.8.1",
    "node-sass": "4.5.2",
    "npm-run-all": "4.0.2",
    "pm2": "2.4.6",
    "react": "15.5.4",
    "react-addons-shallow-compare": "15.5.2",
    "react-dom": "15.5.4",
    "react-github-fork-ribbon": "0.5.0",
    "react-redux": "5.0.4",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "redux-raven-middleware": "1.2.0",
    "redux-thunk": "2.2.0",
    "sass-loader": "6.0.3",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.11.0",
    "webpack": "2.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "chai-enzyme": "0.8.0",
    "chai-immutable": "1.6.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.1.0",
    "husky": "0.14.3",
    "jsdom": "11.1.0",
    "mocha": "3.4.2",
    "nyc": "11.0.3",
    "react-addons-test-utils": "15.6.0",
    "react-hot-loader": "1.3.1",
    "sass-lint": "1.10.2",
    "sinon": "2.4.1",
    "supertest": "3.0.0",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/boggle.git"
  },
  "engines": {
    "node": "8.2.1"
  },
  "author": "Leonardo Quixad√°",
  "license": "BSD"
}

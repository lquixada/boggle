{
  "name": "boggle",
  "version": "0.0.1",
  "description": "Boggle Game",
  "scripts": {
    "deploy": "git push origin master:gh-pages",
    "test": "mocha tests/**/*.spec.js --compilers js:babel-core/register",
    "test:debug": "mocha --debug-brk tests/**/*.spec.js --compilers js:babel-core/register",
    "start": "webpack-dev-server --config config/webpack.config.js --hot --inline",
    "build": "webpack --config config/webpack.config.js",
    "lint": "eslint -c config/.eslintrc **/*.js",
    "publish": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "expect": "^1.18.0",
    "fetch-jsonp": "^1.0.0",
    "jquery": "^2.2.4",
    "jsdom": "^9.2.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "null-loader": "^0.1.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",

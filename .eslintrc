{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": [
    "react",
    "import"
  ],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "block-scoped-var": "off",
    "brace-style": "error",
    "camelcase": "error",
    "comma-dangle": "off",
    "comma-style": ["error", "last"],
    "complexity": ["error", 6],
    "consistent-this": ["error", "that"],
    "curly": ["error", "all"],
    "dot-notation": "error",
    "eol-last": "error",
    "eqeqeq": "off",
    "global-require": "off",
    "guard-for-in": "off",
    "indent": ["error", 2, { "SwitchCase": 1 } ],
    "linebreak-style": "off",
    "max-depth": ["error", 4],
    "max-len": ["error", 120],
    "max-nested-callbacks": ["error", 4],
    "max-params": ["error", 3],
    "max-statements": ["error", 13],
    "new-cap": "off",
    "no-alert": "off",
    "no-bitwise": "error",
    "no-caller": "error",
    "no-cond-assign": "off",
    "no-console": "off",
    "no-debugger": "off",
    "no-empty": "error",
    "no-eq-null": "off",
    "no-eval": "off",
    "no-invalid-this": "off",
    "no-iterator": "off",
    "no-loop-func": "off",
    "no-mixed-spaces-and-tabs": "error",
    "no-multi-str": "error",
    "no-multiple-empty-lines": "error",
    "no-new": "error",
    "no-new-func": "off",
    "no-new-wrappers": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-proto": "off",
    "no-script-url": "off",
    "no-shadow": "off",
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "no-unused-expressions": "off",
    "no-use-before-define": "off",
    "object-curly-spacing": ["error", "never"],
    "one-var": ["error", "never"],
    "quotes": ["error", "single"],
    "require-jsdoc": "off",
    "semi": ["error", "always"],
    "space-before-blocks": ["error", "always"],
    "spaced-comment": ["error", "always"],
    "strict": "off",
    "wrap-iife": "error",
    "yoda": ["error", "never"],
    
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/self-closing-comp": "error",

    "import/first": ["error"],
    "import/newline-after-import": ["error"],
    "import/order": ["error", {"groups": ["builtin", "external"]}],
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
